@{
}

@model IEnumerable<PageTemplate>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>PageCategories</th>
            <th>.Pages</th>
            <th>PageTemplateHistories</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.PageCategories</td>
                <td>@product.Pages</td>
                <td>@product.PageTemplateHistories</td>
            </tr>
        }
    </tbody>
</table>
